{
  // customization
  "vscode_custom_css.imports": [
    "file:///C:/injty/vscode/custom.css",
    "file:///C:/injty/vscode/script.js",
  ],
  // ####################
  // default
  "breadcrumbs.enabled": false,
  "breadcrumbs.showClasses": false,
  "files.eol": "\n",
  "files.autoSaveDelay": 1200,
  "files.insertFinalNewline": false,
  "files.defaultLanguage": "html",
  "outline.collapseItems": "alwaysCollapse",
  "postman.telemetry.enabled": false,
  "files.exclude": {
    "**/.vscode": false,
    "**/.next": true,
    "**/.idea": true,
    "**/node_modules": true,
    "**/.yarn": true,
    "**/.expo": true,
    "**/.expo-shared": true
  },
  "search.exclude": {
    "**/.next": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.idea/": true,
    "**/.yarn/": true,
    "**/tmp": true,
    "**.log": true,
    "**/target": true
  },
  // ####################
  // fonts
  "editor.fontLigatures": "",
  // "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.lineHeight": 1.25,
  "editor.fontWeight": "400",
  "editor.fontFamily": "geist mono",
  "editor.codeLensFontFamily": "geist mono",
  "scm.inputFontFamily": "geist mono",
  "debug.console.fontFamily": "geist mono",
  "notebook.output.fontFamily": "geist mono",
  "chat.editor.fontFamily": "geist mono",
  "editor.inlayHints.fontFamily": "geist mono",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.fastScrollSensitivity": 1,
  "editor.folding": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.glyphMargin": false,
  "editor.guides.bracketPairs": false,
  "editor.guides.indentation": false,
  "editor.hover.enabled": true,
  "editor.insertSpaces": true,
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.minimap.autohide": false,
  "editor.minimap.renderCharacters": false,
  "editor.mouseWheelScrollSensitivity": 3,
  "editor.renderLineHighlight": "gutter",
  "editor.renderWhitespace": "none",
  "editor.quickSuggestions": {
    "other": "off",
    "comments": "off",
    "strings": "off"
  },
  "editor.suggestOnTriggerCharacters": false,
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.semanticHighlighting.enabled": false,
  "editor.smoothScrolling": true,
  "editor.stickyTabStops": true,
  "editor.suggest.insertMode": "replace",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.wordWrap": "off",
  "editor.wordWrapColumn": 160,
  "editor.suggest.preview": true,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "never",
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never",
    "source.sortImports": "explicit",
  },
  "editor.semanticTokenColorCustomizations": {
    "enabled": true
  },
  "editor.stickyScroll.enabled": false,
  "window.title": " ",
  "window.titleSeparator": " â€” ",
  // ####################
  // terminal:
  "terminal.integrated.copyOnSelection": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.fontWeightBold": "400",
  "terminal.integrated.fontFamily": "geist mono",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.lineHeight": 1,
  "terminal.integrated.letterSpacing": 1,
  "terminal.integrated.gpuAcceleration": "auto",
  "terminal.integrated.rightClickBehavior": "selectWord",
  "terminal.integrated.smoothScrolling": true,
  // ####################
  // emmet:
  "emmet.showExpandedAbbreviation": "never",
  "emmet.showSuggestionsAsSnippets": false,
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "vue-html": "html",
    "php": "html"
  },
  // ####################
  // git:
  "git.allowForcePush": true,
  "git.autoStash": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.decorations.enabled": false,
  // ####################
  // configurations:
  "javascript.autoClosingTags": true,
  "javascript.preferences.importModuleSpecifierEnding": "minimal",
  "javascript.preferences.quoteStyle": "double",
  "javascript.suggest.autoImports": false,
  "javascript.suggest.completeJSDocs": true,
  "javascript.suggest.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  // ####################
  // prettier:
  "prettier.enable": true,
  "prettier.requireConfig": true,
  // "prettier.arrowParens": "always",
  // "prettier.bracketSameLine": true,
  // "prettier.bracketSpacing": true,
  // "prettier.endOfLine": "auto",
  // "prettier.jsxSingleQuote": false,
  // "prettier.semi": true,
  // "prettier.singleQuote": false,
  // "prettier.tabWidth": 2,
  // "prettier.trailingComma": "all",
  // "prettier.useTabs": false,
  // "prettier.printWidth": 180,
  // languages
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "typescript.preferences.quoteStyle": "double",
  "typescript.preferences.renameMatchingJsxTags": true,
  "typescript.preferences.useAliasesForRenames": true,
  "typescript.suggestionActions.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "html.completion.attributeDefaultValue": "doublequotes",
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  // ####################
  // zen mode:
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": false,
  "zenMode.hideActivityBar": true,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": true,
  "zenMode.restore": true,
  "zenMode.showTabs": "single",
  "zenMode.silentNotifications": true,
  // ####################
  // vim:
  "vim.easymotion": true,
  "vim.autoindent": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.sneak": true,
  "vim.showcmd": false,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-c>": false,
    "<C-x>": false,
    "<C-v>": false,
    "<C-z>": false,
    "<C-y>": false
  },
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<tab>"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "<S-tab>"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "h"
      ],
      "after": [
        "_"
      ]
    },
    {
      "before": [
        "<leader>",
        "l"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "d"
      ]
    },
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "w"
      ],
      "commands": [
        "workbench.action.splitEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "revealInExplorer"
      ]
    },
    {
      "before": [
        "<leader>",
        "h"
      ],
      "after": [
        "_"
      ]
    },
    {
      "before": [
        "<leader>",
        "l"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "d"
      ]
    }
  ],
  // ####################
  // todo highlight
  "todohighlight.isCaseSensitive": true,
  "todohighlight.isEnable": true,
