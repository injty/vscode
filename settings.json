      "backgroundColor": "#D24343"
    },
    {
      "text": "[ ]",
      "color": "#fff",
      "backgroundColor": "#f7c021"
    },
    {
      "text": "[x]",
      "color": "#fff",
      "backgroundColor": "#387de1"
    },
    {
      "text": "[>]",
      "color": "#fff",
      "backgroundColor": "#92c353"
    },
    {
      "text": "[-]",
      "color": "#fff",
      "backgroundColor": "#ce472c"
    }
  ],
